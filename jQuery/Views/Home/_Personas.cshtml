@model IEnumerable<jQuery.Controllers.Persona>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Edad</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.nombre</td>
                <td>@item.edad</td>
            </tr>
        }
    </tbody>
</table>
